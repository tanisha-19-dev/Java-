package Learn;
import java.util.Scanner;

public class table {
    public static void main(String[] args) {
        System.out.println("Enter a number : ");
        Scanner S = new Scanner (System.in);
        int a = S.nextInt();
        int i = 1;
        System.out.println("Multiplication table is : \n ");
        while (i<=10){

            System.out.println(a+"*"+i+"="+a*i);
            i++;

        }

    }

}
