package Learn;

public class Star {
    public static void main(String[] args) {

        System.out.println("Here is the Pattern :\n");

        for (int i=1 ;i<=5;i++){
            for (int j=0;j<i;j++) {

                System.out.print("*");
            }
            System.out.print("\n");
        }
    }
}
